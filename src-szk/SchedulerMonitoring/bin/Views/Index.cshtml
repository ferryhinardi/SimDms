@{ViewBag.Title = @Model.Name;}
<!doctype html>
<html>
	<head>
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
		<meta charset="utf-8" />	
		<title>Hello @ViewBag.Title</title>
		<meta name="description" content="Home Application" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />	
		
		<!-- bootstrap & fontawesome -->
		<link href="css/bootstrap.min.css" rel="stylesheet" />    
		<link href="css/font-awesome.min.css" rel="stylesheet" />    
		<link href="themes/classic/ext-theme-classic-all.css" rel="stylesheet" />    
		
		<script src="js/jquery-2.1.1.min.js"></script>  
		<script src="js/ext-all.js"></script>  
		<script src="js/bootstrap.min.js"></script>  
		<script src="js/ext-theme-classic.js"></script>  
	</head>
	<body>
<div class="sidebar-shortcuts" id="sidebar-shortcuts">
					<div class="sidebar-shortcuts-large" id="sidebar-shortcuts-large">
						<button class="btn btn-success">
							<i class="ace-icon fa fa-signal"></i>
						</button>

						<button class="btn btn-info">
							<i class="ace-icon fa fa-pencil"></i>
						</button>

						<!-- #section:basics/sidebar.layout.shortcuts -->
						<button class="btn btn-warning">
							<i class="ace-icon fa fa-users"></i>
						</button>

						<button class="btn btn-danger">
							<i class="ace-icon fa fa-cogs"></i>
						</button>

						<!-- /section:basics/sidebar.layout.shortcuts -->
					</div>

					<div class="sidebar-shortcuts-mini" id="sidebar-shortcuts-mini">
						<span class="btn btn-success"></span>

						<span class="btn btn-info"></span>

						<span class="btn btn-warning"></span>

						<span class="btn btn-danger"></span>
					</div>
				</div><!-- /.sidebar-shortcuts -->
				Email: <input type='text' value='@Model.Email'></input>
	</body>
</html>
